@model IEnumerable<myDoctor.Models.KhachHang>

@{
    ViewBag.Title = "taikhoanDK";
    Layout = "~/Views/Shared/LayoutAdmin.cshtml";
}

<div id="page-wrapper">
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">Khách hàng</h1>
            </div>
            <!-- /.col-lg-12 -->
        </div>
        <!-- /.row -->
        <div class="row">
            <div class="col-lg-12">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        Danh sách thông tin tài khoản khách hàng
                    </div>
                    <!-- /.panel-heading -->
                    <div class="panel-body">
                        <div class="table-responsive">
                            <table class="table table-striped table-bordered table-hover" id="dataTables-example">
                                <thead>
                                    <tr>
                                        <th>ID tài khoản</th>
                                        <th>Tên tài khoản</th>
                                        <th>Số điện thoại</th>
                                        <th>Email</th>
                                        <th>Tuổi</th>
                                        <th>Chi tiết</th>

                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var i in Model)
                                    {
                                    <tr class="gradeU">
                                        <td>@i.idKhachHang</td>
                                        <td>@i.tenkh</td>
                                        <td>@i.sdt</td>
                                        <td>@i.email</td>
                                        <td>@i.tuoi</td>
                                        <td>
                                            <a href="@Url.Action("xemthongtin","admin",new {id =i.idKhachHang})" style="color: #B22428 ">Xem</a>
                                        </td>
                                    </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                        <!-- /.table-responsive -->

                    </div>
                    <!-- /.panel-body -->
                </div>
                <!-- /.panel -->
            </div>
            <!-- /.col-lg-12 -->
        </div>

        <!-- /.row -->
        <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
</div>